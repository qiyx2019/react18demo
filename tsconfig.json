{
  "compilerOptions": {
      "outDir": "dist",
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "ES5",
      "jsx": "react",
      "rootDirs": ["src"],
      "baseUrl": ".",
      "declaration": true,      
      "esModuleInterop": true,     
      "skipLibCheck": true,                     /* Skip type checking of declaration files. */
      "forceConsistentCasingInFileNames": true,              
      "paths": {
        // 映射列表
        "@/*": [
          "src/*"
        ],
      }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*","global.d.ts"]
}